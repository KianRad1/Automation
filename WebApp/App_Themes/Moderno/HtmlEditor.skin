<%@ Register TagPrefix="dx" Namespace="DevExpress.Data" Assembly="DevExpress.Data.v13.2, Version=13.2.5.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" %>
<%@ Register Assembly="DevExpress.Web.ASPxHtmlEditor.v13.2, Version=13.2.5.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" Namespace="DevExpress.Web.ASPxHtmlEditor" TagPrefix="dxhe" %>
<%@ Register Assembly="Intek.WebControls" Namespace="Intek.WebControls"
    tagprefix="Intek" %>
<Intek:HtmlEditor runat="server" >
    <SettingsLoadingPanel Text="" />
    <Styles CssPostfix="Moderno" CssFilePath="~/App_Themes/Moderno/{0}/styles.css">
        <ViewArea>
            <Border BorderStyle="Solid" BorderColor="#A3C0E8" BorderWidth="1px" />
        </ViewArea>
    </Styles>
    <StylesToolbars>
        <Toolbar ItemSpacing="2px" />
    </StylesToolbars>
    <StylesStatusBar>
        <ActiveTab BackColor="White" />
    </StylesStatusBar>
    <Images ImageFolder="~/App_Themes/Moderno/{0}/"></Images>
    <ImagesEditors>
        <DropDownEditDropDown Width="7px" Height="7px" Url="~/App_Themes/Moderno/HtmlEditor/tbDropDown.png" UrlHottracked="~/App_Themes/Moderno/HtmlEditor/tbDropDownHottracked.png" UrlDisabled="~/App_Themes/Moderno/HtmlEditor/tbDropDownDisabled.png" />
        <SpinEditLargeIncrement Url="~/App_Themes/Moderno/Editors/edtSpinEditLargeIncImage.png" UrlHottracked="~/App_Themes/Moderno/Editors/edtSpinEditLargeIncHottrackedImage.png" UrlPressed="~/App_Themes/Moderno/Editors/edtSpinEditLargeIncHottrackedImage.png" UrlDisabled="~/App_Themes/Moderno/Editors/edtSpinEditLargeIncDisabledImage.png" Width="7px" Height="9px" />
        <SpinEditDecrement Url="~/App_Themes/Moderno/Editors/edtSpinEditDecrementImage.png" UrlHottracked="~/App_Themes/Moderno/Editors/edtSpinEditDecrementHottrackedImage.png" UrlPressed="~/App_Themes/Moderno/Editors/edtSpinEditDecrementHottrackedImage.png" UrlDisabled="~/App_Themes/Moderno/Editors/edtSpinEditDecrementDisabledImage.png" Width="7px" Height="7px" />
        <SpinEditLargeDecrement Url="~/App_Themes/Moderno/Editors/edtSpinEditLargeDecImage.png" UrlHottracked="~/App_Themes/Moderno/Editors/edtSpinEditLargeDecHottrackedImage.png" UrlPressed="~/App_Themes/Moderno/Editors/edtSpinEditLargeDecHottrackedImage.png" UrlDisabled="~/App_Themes/Moderno/Editors/edtSpinEditLargeDecDisabledImage.png" Width="7px" Height="9px" />
        <SpinEditIncrement Url="~/App_Themes/Moderno/Editors/edtSpinEditIncrementImage.png" UrlHottracked="~/App_Themes/Moderno/Editors/edtSpinEditIncrementHottrackedImage.png" UrlPressed="~/App_Themes/Moderno/Editors/edtSpinEditIncrementHottrackedImage.png" UrlDisabled="~/App_Themes/Moderno/Editors/edtSpinEditIncrementDisabledImage.png" Width="7px" Height="7px" />
    </ImagesEditors>
    <PartsRoundPanel>
        <TopLeftCorner Height="5px" Width="5px" Url="~/App_Themes/Moderno/HtmlEditor/RoundPanel/herpTopLeft.png" />
        <NoHeaderTopLeftCorner Height="5px" Width="5px" Url="~/App_Themes/Moderno/HtmlEditor/RoundPanel/herpNoHeaderTopLeft.png" />
        <TopRightCorner Height="5px" Width="5px" Url="~/App_Themes/Moderno/HtmlEditor/RoundPanel/herpTopRight.png" />
        <NoHeaderTopRightCorner Height="5px" Width="5px" Url="~/App_Themes/Moderno/HtmlEditor/RoundPanel/herpNoHeaderTopRight.png" />
        <BottomLeftCorner Height="5px" Width="5px" Url="~/App_Themes/Moderno/HtmlEditor/RoundPanel/herpBottomLeft.png" />
        <BottomRightCorner Height="5px" Width="5px" Url="~/App_Themes/Moderno/HtmlEditor/RoundPanel/herpBottomRight.png" />
        <%-- Header parts --%>
        <HeaderContent>
            <BackgroundImage Repeat="RepeatX" VerticalPosition="Top" ImageUrl="~/App_Themes/Moderno/HtmlEditor/RoundPanel/herpHeaderBackground.gif" />
        </HeaderContent>
        <HeaderLeftEdge>
            <BackgroundImage Repeat="NoRepeat" VerticalPosition="Top" ImageUrl="~/App_Themes/Moderno/HtmlEditor/RoundPanel/herpHeaderLeftEdge.gif" />
        </HeaderLeftEdge>
        <HeaderRightEdge>
            <BackgroundImage Repeat="NoRepeat" ImageUrl="~/App_Themes/Moderno/HtmlEditor/RoundPanel/herpHeaderRightEdge.gif" VerticalPosition="Top" />
        </HeaderRightEdge>
        <%-- Middle parts --%>
        <TopEdge>
            <BackgroundImage Repeat="RepeatX" VerticalPosition="Top" ImageUrl="~/App_Themes/Moderno/HtmlEditor/RoundPanel/herpTopEdge.gif" />
        </TopEdge>
        <NoHeaderTopEdge BackColor="#FFFFFF">
            <BackgroundImage Repeat="RepeatX" VerticalPosition="Top" ImageUrl="~/App_Themes/Moderno/HtmlEditor/RoundPanel/herpNoHeaderTopEdge.gif" />
        </NoHeaderTopEdge>
        <BottomEdge>
            <BackgroundImage Repeat="RepeatX" VerticalPosition="Top" ImageUrl="~/App_Themes/Moderno/HtmlEditor/RoundPanel/herpBottomEdge.gif" />
        </BottomEdge>
        <LeftEdge>
            <BackgroundImage Repeat="RepeatY" VerticalPosition="Top" ImageUrl="~/App_Themes/Moderno/HtmlEditor/RoundPanel/herpLeftEdge.gif" />
        </LeftEdge>
        <RightEdge>
            <BackgroundImage Repeat="RepeatY" VerticalPosition="Top" ImageUrl="~/App_Themes/Moderno/HtmlEditor/RoundPanel/herpRightEdge.gif" />
        </RightEdge>    </PartsRoundPanel>
    <StylesRoundPanel>            
        <ControlStyle BackColor="#FFFFFF">
            <Border BorderStyle="None" />
        </ControlStyle>
    </StylesRoundPanel>    
</Intek:HtmlEditor>
